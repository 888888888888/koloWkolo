        </activity>
        <activity
            android:name=".PlayerActivity"
            android:label="KolkoApp"
            />
        <activity
            android:name=".PlayerActivity2"
            android:label="KolkoApp"
            />
        <activity
            android:name=".SearchActivity"
            android:label="KolkoApp"
            />
        <activity
            android:name=".SearchActivity2"
            android:label="KolkoApp"
            />
    </application>

</manifest>
